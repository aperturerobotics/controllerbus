{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    ":semanticPrefixFixDepsChoreOthers",
    ":ignoreModulesAndTests",
    "group:all",
    "workarounds:all"
  ],
  "branchConcurrentLimit": 0,
  "ignorePaths": [
    "hack",
    "examples/plugin-demo"
  ],
  "packageRules": [{
    "matchManagers": ["gomod"],
    "matchDepTypes": ["replace"],
    "enabled": false
  }]
}
