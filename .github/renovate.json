{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    ":dependencyDashboard",
    ":semanticPrefixFixDepsChoreOthers",
    ":ignoreModulesAndTests",
    "group:all",
    "workarounds:all"
  ],
  "branchConcurrentLimit":0,
  "packageRules": [
    {
      "matchLanguages": ["go"],
      "groupName": "go"
    },
    {
      "matchLanguages": ["js"],
      "groupName": "js"
    }
  ]
}
